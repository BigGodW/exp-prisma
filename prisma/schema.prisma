generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model user {
  id   Int     @id
  name String? @db.VarChar(255)
  age  Int?
  message String? 
} 

model Habit{
  id Int @id @default(autoincrement())
  title String 
  description String?
  createdAt DateTime @default(now())
  isEnd Boolean @default(false)
  endTime DateTime?
}
model Tag{
  id Int @id @default(autoincrement())
  title String
}